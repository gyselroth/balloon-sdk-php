{
    "name": "gyselroth/balloon-sdk",
    "description": "Official balloon sdk for php",
    "keywords": ["balloon", "sdk", "swagger", "openapi"],
    "homepage": "http://www.github.com/gyselroth/balloon-sdk-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Raffael Sahli",
            "email": "sahli@gyselroth.net"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "friendsofphp/php-cs-fixer": "*",
        "phpstan/phpstan": "*"
    },
    "autoload": {
        "psr-4": { "Balloon\\Sdk\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Balloon\\Sdk\\" : "test/" }
    }
}
